{
    "name": "snipon/wordpress",
    "type": "project",
    "authors": [
        {
            "name": "Simon Larsson",
            "email": "me@simon-larsson.se"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "dac/dac-content-hub",
                "version": "1.0",
                "type": "wordpress-plugin",
                "source": {
                    "url": "git@github.com:Socialsquare/wp-dac-content-hub.git",
                    "type": "git",
                    "reference": "origin/master"
                },
                "require": {
                    "prismic/php-sdk": "^3",
                    "composer/installers": "~1.0",
                    "twig/twig": "^2.4"
                }
            }
        }
    ],
    "require": {
        "php": "^7.1",
        "johnpbloch/wordpress": "^4.8",
        "wpackagist-plugin/debug-bar": "^0.9.0",
        "dac/dac-content-hub": "1.0",
        "wpackagist-plugin/kint-debugger": "^1.2",
        "wpackagist-theme/twentyseventeen": "^1.3"
    },
    "extra": {
        "wordpress-install-dir": "public/wp",
        "installer-paths": {
            "public/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "public/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "public/wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
